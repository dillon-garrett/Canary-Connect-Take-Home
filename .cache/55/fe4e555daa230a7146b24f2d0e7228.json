e":"setReadingToDisplay","original":{"line":41,"column":6},"generated":{"line":1,"column":532}},{"source":"../src/Utils/fetchGraphData.js","name":"tempArr","original":{"line":41,"column":26},"generated":{"line":1,"column":534}},{"source":"../src/Utils/fetchGraphData.js","name":"catch","original":{"line":43,"column":5},"generated":{"line":1,"column":539}},{"source":"../src/Utils/fetchGraphData.js","original":{"line":43,"column":11},"generated":{"line":1,"column":545}},{"source":"../src/Utils/fetchGraphData.js","original":{"line":44,"column":12},"generated":{"line":1,"column":556}},{"source":"../src/Utils/fetchGraphData.js","original":{"line":44,"column":12},"generated":{"line":1,"column":562}},{"source":"../src/Utils/fetchGraphData.js","name":"Error","original":{"line":44,"column":16},"generated":{"line":1,"column":566}},{"source":"../src/Utils/fetchGraphData.js","original":{"line":44,"column":22},"generated":{"line":1,"column":572}},{"source":"../src/Utils/fetchGraphData.js","name":"fetchGraphData","original":{"line":48,"column":15},"generated":{"line":1,"column":612}},{"source":"../src/Utils/fetchGraphData.js","name":"fetchGraphData","original":{"line":48,"column":15},"generated":{"line":1,"column":614}},{"source":"../src/Utils/fetchGraphData.js","name":"fetchGraphData","original":{"line":48,"column":15},"generated":{"line":1,"column":616}},{"source":"../src/Utils/fetchGraphData.js","name":"fetchGraphData","original":{"line":48,"column":15},"generated":{"line":1,"column":624}},{"source":"../src/Utils/fetchGraphData.js","name":"fetchGraphData","original":{"line":48,"column":15},"generated":{"line":1,"column":632}}],"sources":{"../src/Utils/fetchGraphData.js":"const fetchGraphData = (proxy, url, setHumidity, setTemp, setAirQuality, setReadingToDisplay) => {\n  fetch(proxy + url, {\n    method: 'GET',\n    headers: { 'Content-Type': 'application/json' }\n    //   mode: 'no-cors'\n  })\n    .then(res => res.json())\n    .then(response => {\n      // initializing arrays to set state\n      const humidArr = [];\n      const tempArr = [];\n      const airArr = [];\n      response.forEach(el => {\n        // since the graph payload type needs to be formatted as an array of objects, initializing object to push to array\n        const obj = {};\n        // determining which reading to grab from the API\n        if (el.type === 'humidity') {\n          // grabbing the created at and value props\n          obj.createdAt = el.createdAt;\n          obj.value = el.value;\n          humidArr.push(obj);\n        }\n        if (el.type === 'temperature') {\n          // grabbing the created at and value props\n          obj.createdAt = el.createdAt;\n          obj.value = el.value;\n          tempArr.push(obj);\n        }\n        if (el.type === 'airquality') {\n          // grabbing the created at and value props\n          obj.createdAt = el.createdAt;\n          obj.value = el.value;\n          airArr.push(obj);\n        }\n      });\n      // setting the custom hooks to be used upon submission\n      setHumidity(humidArr);\n      setTemp(tempArr);\n      setAirQuality(airArr);\n      // set the defualt display to be temperature\n      setReadingToDisplay(tempArr);\n    })\n    .catch(() => {\n      throw new Error('error in fetching device readings');\n    });\n};\n\nexport default fetchGraphData;\n"},"lineCount":null}},"error":null,"hash":"d7bbb67ce025bd88ddeb878ffbbc5bbe","cacheData":{"env":{}}}